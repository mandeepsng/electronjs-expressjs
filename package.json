{
  "name": "rvs",
  "version": "1.0.0",
  "description": "desktop application with nodejs",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-builder",
    "dev": "electron .",
    "start": "electron-forge start",
    "build": "electron-builder",
    "build:win": "electron-builder build --win",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Mandeep Singh",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "csv-parser": "^3.0.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "hbs": "^4.2.0",
    "html-pdf": "^3.0.1",
    "multer": "^1.4.5-lts.1"
  },
  "build": {
    "appId": "com.example.app",
    "asar": false,
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "assets/icon.png"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "assets/icon.png"
    }
  },
  "devDependencies": {
    "child_process": "^1.0.2",
    "electron": "^24.3.0",
    "electron-builder": "^23.6.0"
  }
}
